{
  "name": "tabstein",
  "version": "0.7.3",
  "main": "dist/main.js",
  "repository": "git@github.com:jhegele/TabStein.git",
  "author": "John Hegele",
  "license": "MIT",
  "scripts": {
    "types:check": "npx tsc --noEmit --skipLibCheck",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.dev.js --display-error-details",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "pack:test:mac": "electron-builder --mac --config ./electron-builder.config.dev.json",
    "pack:release:mac": "electron-builder -m --config ./electron-builder.config.prod.json",
    "pack:test:win": "electron-builder --win --config ./electron-builder.config.dev.json",
    "pack:release:win": "electron-builder --win --config ./electron-builder.config.prod.json",
    "pack:clean:mac": "rimraf ./releases/*.dmg* ./releases/mac",
    "pack:clean:win": "rimraf ./releases/*.exe* ./releases/win-unpacked",
    "release:mac": "npm run types:check && npm run build:prod && npm run pack:clean:mac && npm run pack:release:mac",
    "release:win": "npm run types:check && npm run build:prod && npm run pack:clean:win && npm run pack:release:win",
    "docs:test": "npx docsify serve ./docs",
    "dev": "electron ."
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@types/glob": "^7.1.1",
    "@types/ini": "^1.3.30",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.1",
    "@types/node-fetch": "^2.5.7",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.4",
    "@types/redux-logger": "^3.0.7",
    "archiver": "^4.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "docsify-cli": "^4.4.0",
    "electron": "^8.2.2",
    "electron-builder": "^22.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@emotion/core": "^10.0.28",
    "@reduxjs/toolkit": "^1.3.5",
    "antd": "^4.1.4",
    "axios": "^0.19.2",
    "dayjs": "^1.8.18",
    "glob": "^7.1.6",
    "ini": "^1.3.5",
    "lodash": "^4.17.15",
    "python-shell": "^2.0.0",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6"
  }
}
